    "acorn-walk": "^8.3.4",
    "astring": "^1.9.0",
    "fs": "^0.0.1-security",
    "glob": "^11.0.3"
  },
  "devDependencies": {
    "@hotwired/stimulus": "^3.2.2",
    "@types/node": "^24.9.1",
    "@typescript-eslint/types": "^8.46.2",
    "@vitest/ui": "^4.0.3",
    "dedent": "^1.7.0",
    "minimatch": "^10.0.3",
    "path": "^0.12.7",
    "rimraf": "^6.0.1",
    "source-map": "^0.7.6",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.3"
  }
