{
  "name": "stimulus-parser",
  "version": "0.3.0",
  "description": "Statically analyze Stimulus controllers",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "Marco Roth",
  "license": "MIT",
  "repository": "https://github.com/marcoroth/stimulus-parser",
  "sideEffects": false,
  "scripts": {
    "prebuild": "yarn clean",
    "build": "tsc -b",
    "watch": "tsc -b -w",
